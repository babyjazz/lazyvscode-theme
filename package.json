{
  "name": "babyjazz",
  "displayName": "LazyVscode Theme",
  "description": "Fancy, smooth and beautiful UI like Lazyvim",
  "version": "2.0.0",
  "publisher": "babyjazz",
  "repository": {
    "url": "https://github.com/babyjazz/lazyvscode-theme"
  },
  "icon": "logo.png",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other",
    "Themes"
  ],
  "preview": true,
  "extensionKind": [
    "ui"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "babyjazz.enable-lazyvscode-theme",
        "title": "Enable/Update LazyVscode Theme"
      },
      {
        "command": "babyjazz.disable-lazyvscode-theme",
        "title": "Disable LazyVscode Theme"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "pnpm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.50.5",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.25.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  }
}
